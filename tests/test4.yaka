PROGRAMME param
ENTIER FONCTION max (ENTIER i, ENTIER j)
VAR BOOLEEN b,c;
VAR ENTIER k;
	k=i;
	SI k>j ALORS RETOURNE k
	SINON RETOURNE j FSI
FFONCTION

ENTIER FONCTION min (ENTIER i, ENTIER j)
	SI i<j ALORS RETOURNE i
	SINON RETOURNE j FSI
FFONCTION

BOOLEEN FONCTION sup(ENTIER i, ENTIER j)
	RETOURNE i>j
FFONCTION

PRINCIPAL
VAR ENTIER i,j,k;
VAR BOOLEEN b, c, d;
	i=5;LIRE(j);ALALIGNE;
	k=max (i,min(j,5))+2;
	b=sup(1+max(i,j-5),
		min(i*2,j));
	c = sup(2,5);
	d = sup(5,2);
	ALALIGNE;
	ECRIRE(k);
	ALALIGNE;
	ECRIRE(b);
	ALALIGNE;
	ECRIRE(c);
	ALALIGNE;
	ECRIRE(d);
FPRINCIPAL

FPROGRAMME